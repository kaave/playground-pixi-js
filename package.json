{
  "name": "vue-template",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dashboard -c cyan -- npm run start:plain",
    "start:plain": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --history-api-fallback --progress --color --config tools/webpack/development.js",
    "build": "run-s clean build:source",
    "build:source": "cross-env NODE_ENV=production webpack --progress --config tools/webpack/production.js",
    "build:check": "run-s build server",
    "build:diff": "node tools/getBuildDiff",
    "clean": "rimraf build",
    "server": "cross-env NODE_ENV=production http-server ./build -p 23000 -o --cors",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "safe-guard": "lint-staged",
    "fix": "run-p fix:*",
    "fix:style": "prettier --write 'src/**/*.css'",
    "fix:script": "tslint --fix 'src/**/*.{ts,tsx}'",
    "fix:json": "prettier --write 'src/**/*.json'",
    "lint": "run-p lint:*",
    "lint:style": "stylelint 'src/**/*.css'",
    "lint:script": "tslint 'src/**/*.{ts,tsx}'",
    "precommit": "run-s safe-guard",
    "tsc": "tsc"
  },
  "browserslist": [
    "> 1% in JP"
  ],
  "lint-staged": {
    "subTaskConcurrency": 1,
    "linters": {
      "*.ts": [
        "tslint --fix",
        "git add"
      ],
      "*.css": [
        "prettier --write",
        "stylelint",
        "git add"
      ],
      "*.json": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "autoprefixer": "^7.2.6",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "globby": "^7.1.1",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.1.0",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-webpack-plugin": "^2.0.0",
    "jest": "^22.2.2",
    "lint-staged": "^6.1.0",
    "npm-run-all": "^4.1.2",
    "postcss-color-hex-alpha": "^3.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-simple-vars": "^4.1.0",
    "postcss-url": "^7.3.0",
    "prettier": "^1.10.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.1",
    "stylelint": "^8.4.0",
    "stylelint-config-framelunch": "framelunch/stylelint-config-framelunch#v0.3.0",
    "stylelint-processor-html": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^22.0.4",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.7.1",
    "url-loader": "^0.6.2",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "libraries-frontend-framelunch": "framelunch/libraries-frontend-framelunch#0.3.2",
    "sanitize.css": "^5.0.0",
    "tslib": "^1.9.1"
  }
}
